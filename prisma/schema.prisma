// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model Usuario{
  id Int @id @default(autoincrement())
  idPerfil Int @unique
  idConta Int @unique
  idExerSegunda Int @unique
  idExerTerca Int @unique
  idExerQuarta Int @unique
  idExerQuinta Int @unique
  idExerSexta Int @unique
  idExerSabado Int @unique
  idExerDomingo Int @unique
  idImc Int @unique
  idJantar Int @unique
  idCafe Int @unique
  idLancheManha Int @unique
  idAlmoco Int @unique
  idLancheTarde Int @unique


  imc IMC @relation(fields: [idImc], references: [id])
  jantar Jantar @relation(fields: [idJantar], references: [id])
  cafe Cafe @relation(fields: [idCafe], references: [id])
  lancheManha Lanche_manha @relation(fields: [idLancheManha], references: [id])
  almoco Almoco @relation(fields: [idAlmoco], references: [id])
  lancheTarde Lanche_tarde @relation(fields: [idLancheTarde], references: [id])
  exerSegunda ExerSegunda  @relation(fields: [idExerSegunda], references: [id])
  exerTerca ExerTerca @relation(fields: [idExerTerca], references: [id])
  exerQuarta ExerQuarta @relation(fields: [idExerQuarta], references: [id])
  exerQuinta ExerQuinta @relation(fields: [idExerQuinta], references: [id])
  exerSexta ExerSexta @relation(fields: [idExerSexta], references: [id])
  exerSabado ExerSabado @relation(fields: [idExerSabado], references: [id])
  exerDomingo ExerDomingo  @relation(fields: [idExerDomingo], references: [id])
  conta Conta @relation(fields: [idConta], references: [id])
  perfil Perfil @relation(fields: [idPerfil], references: [id])

}

model Conta{
  id Int @id @default(autoincrement())
  email String 
  senha String
  nome String
  usuario Usuario?

}

model Perfil{
  id Int @id @default(autoincrement())
  idade String
  peso String
  altura String
  genero String
  fatorAtividade String
  usuario Usuario?
}

model IMC{
  id Int @id @default(autoincrement())
  valor Float
  usuario Usuario?
}



model Jantar{
  id Int @id @default(autoincrement())
  proteina Float
  carboidrato Float
  gordura Float
  nome_alimento String
  quantidade Float
  usuario Usuario?
  
}

model Cafe{
  id Int @id @default(autoincrement())
  proteina Float
  carboidrato Float
  gordura Float
  nome_alimento String
  quantidade Float
  usuario Usuario?

}

model Lanche_manha{
  id Int @id @default(autoincrement())
  proteina Float
  carboidrato Float
  gordura Float
  nome_alimento String
  quantidade Float
  usuario Usuario?
}

model Almoco{
  id Int @id @default(autoincrement())
  proteina Float
  carboidrato Float
  gordura Float
  nome_alimento String
  quantidade Float
  usuario Usuario?
  
}

model Lanche_tarde{
  id Int @id @default(autoincrement())
  proteina Float
  carboidrato Float
  gordura Float
  nome_alimento String
  quantidade Float
  usuario Usuario?
 
}


model ExerSegunda{ //exer_triceps
  id Int @id @default(autoincrement())
  nome String
  serie Int
  repeticao Int
  usuario Usuario?
}

