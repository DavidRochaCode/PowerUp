// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}



model Conta{
  id Int @id @default(autoincrement())
  email String @unique
  senha String
  nome String
  perfil Perfil?
  imc IMC?
  jantar Jantar[]
  cafe Cafe[]
  lanche_manha Lanche_manha[]
  almoco Almoco[]
  lanche_tarde Lanche_tarde[]
  exer_segunda ExerSegunda[]
  exer_terca ExerTerca[]
  exer_quarta ExerQuarta[]
  exer_quinta ExerQuinta[]
  exer_sexta ExerSexta[]
  exer_sabado ExerSabado[]
  exer_domingo ExerDomingo[]
}

model Perfil{
  id Int @id @default(autoincrement())
  idade String
  peso String
  altura String
  genero String
  fatorAtividade String
  id_conta Int @unique
  conta Conta @relation(fields: [id_conta], references: [id])
 
}

model IMC{
  id Int @id @default(autoincrement())
  valor Float
  id_conta Int @unique
  conta Conta @relation(fields: [id_conta], references: [id])
  
}



model Jantar{
  id Int @id @default(autoincrement())
  proteina String?
  carboidrato String?
  gordura String?
  nome_alimento String?
  quantidade String?
  id_conta Int 
  conta Conta @relation(fields: [id_conta], references: [id])
 
  
}

